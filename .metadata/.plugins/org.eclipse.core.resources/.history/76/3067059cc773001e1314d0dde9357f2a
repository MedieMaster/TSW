
public class Mitarbeiter {

	private int mNr;
	private String vorname;
	private String nachname;
	private int alter;
	private String adresse;
	private String bereich;
	
	public Mitarbeiter(int mNr, String vorname, String nachname, String adresse, String bereich, int alter) {
		this.mNr = mNr;
		this.vorname = vorname;
		this.nachname = nachname;
		this.adresse = adresse;
		this.bereich = bereich;
		this.alter = alter;
	}

	public String toString() {
		return "mNr: " + mNr + " | vorname: " + vorname + " | nachname: " + nachname + " | alter: " + alter
				+ " | adresse: " + adresse + " | bereich: " + bereich;
	}

	public int getmNr() {
		return mNr;
	}

	public void setmNr(int mNr) {
		this.mNr = mNr;
	}

	public String getVorname() {
		return vorname;
	}

	public void setVorname(String vorname) {
		this.vorname = vorname;
	}

	public String getNachname() {
		return nachname;
	}

	public void setNachname(String nachname) {
		this.nachname = nachname;
	}

	public int getAlter() {
		return alter;
	}

	public void setAlter(int alter) {
		this.alter = alter;
	}

	public String getAdresse() {
		return adresse;
	}

	public void setAdresse(String adresse) {
		this.adresse = adresse;
	}

	public String getBereich() {
		return bereich;
	}

	public void setBereich(String bereich) {
		this.bereich = bereich;
	}
	
	
	
}
